{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/styles/*": [
        "styles/*"
      ],
      "@/atoms/*": [
        "src/components/atoms/*"
      ],
      "@/molecules/*": [
        "src/components/molecules/*"
      ],
      "@/organisms/*": [
        "src/components/organisms/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/contexts/*": [
        "src/contexts/*"
      ],
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}