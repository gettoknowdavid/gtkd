{
  "name": "gtkd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:tailwind": "concurrently \"next dev\" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i src/styles/global.css -o src/styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@graphcms/rich-text-react-renderer": "^0.6.1",
    "@next/font": "latest",
    "clsx": "^1.2.1",
    "concurrently": "^7.6.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "gsap": "^3.11.3",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-loading-skeleton": "^3.1.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "autoprefixer": "^10.4.13",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.4",
    "typescript": "4.8.4"
  }
}